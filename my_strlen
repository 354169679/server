#include <stdio.h>

size_t my_strlen(const char *s)
{
    size_t len = 0;
    if (s == NULL)
        return len;

    while ((*s++) != '\0')
    {
        len++;
    }
    return len;
}

void mycat(char *des, const char *src)
{
    while (*des++)
        ;
    des--;
    while (*des++ = *src++)
    {
        ;
    }
}


int main(int argc, char *argv[])
{
    const char *str = "hello";
    printf("%lu", my_strlen(str));
    return 0;
}
